<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <Match>
        <Bug code="EI, EI2, CT"/>
    </Match>
    <Match>
        <Bug code="FS"/>
        <Or>
            <Class name="com.varlanv.testkonvence.commontest.TestSamples"/>
            <Class name="com.varlanv.testkonvence.gradle.plugin.TestKonvencePluginFunctionalTest"/>
        </Or>
    </Match>
    <Match>
        <Bug code="NP"/>
        <Method name="equals"/>
    </Match>
</FindBugsFilter>
